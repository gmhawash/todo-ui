{
  "name": "todo-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean-dist": "./node_modules/.bin/rimraf ./dist && mkdir dist",
    "prebuild": "./node_modules/.bin/npm-run-all clean-dist lint",
    "build": "node build/build.js",
    "lint": "./node_modules/.bin/eslint build src",
    "create-mock-data": "node build/api/createMockData.js",
    "prestart-mockapi": "npm run create-mock-data",
    "start-mockapi": "./node_modules/.bin/json-server --watch build/api/db.json --port 3000",
    "live-server": "./node_modules/.bin/live-server src --mount=/axios:node_modules/axios/dist --mount=/vue:node_modules/vue/dist --mount=/bulma:node_modules/bulma/css --proxy=/tasks:http://localhost:3000/tasks",
    "start": "npm-run-all --parallel start-mockapi live-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "bulma": "^0.5.3",
    "npm-run-all": "^4.1.1",
    "vue": "^2.4.4"
  },
  "devDependencies": {
    "chalk": "^2.1.0",
    "eslint": "^4.8.0",
    "json-schema-faker": "0.4.6",
    "json-server": "^0.12.0",
    "live-server": "^1.2.0",
    "rimraf": "^2.6.2"
  }
}
